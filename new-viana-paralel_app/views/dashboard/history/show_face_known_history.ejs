<div class="tab-pane fade show active" id="show-face-known-history" role="tabpanel" aria-labelledby="home-tab">
    <!-- DataTales Example -->
    <div class="card shadow mb-4 mt-2">
      <div class="card-body">
        <div class="table-responsive">
          <table class="table table-bordered " id="table_face_known" width="100%" cellspacing="0">
            <thead>
              <tr>
                <th>No</th>
                <th>Timestamp</th>
                <th>CCTV</th>
                <th>Name</th>
                <th>Status</th>
                <th>Preview</th>
                <th>Aksi</th>
              </tr>
            </thead>
            <tbody>
              <% for(let i=0; i<face_known.length; i++){ %>
                <tr>
                  <td><%= i+1 %></td>
                  <td><%= face_known[i].face.date %></td>
                  <td><%= face_known[i].event.camera_name %></td>
                  <td><%= face_known[i].face.name %></td>
                  <td><%= face_known[i].face.class  %></td>
                  <td><img src="/images/face-recognition-data/<%= face_known[i].face.images %>" alt="" height="100"></td>
                  <td>
                    <form action="/dashboard/face_known-enrollment/<%= face_known[i].id %>?_method=DELETE" method="POST">
                      <a href="/dashboard/face_known-enrollment/<%= face_known[i].id %>" class="btn btn-warning btn-circle btn-sm button-update" data-id="<%= face_known[i].id %>"><i class="fas fa-edit"></i></a>
                      <button type="submit" class="btn btn-danger btn-circle btn-sm"><i class="fas fa-trash"></i></button>
                    </form>
                  </td>
                </tr>
              <%} %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  