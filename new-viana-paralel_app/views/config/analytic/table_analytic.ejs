<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
      
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
          <thead>
            <tr>
              <th>No</th>
              <th>Name</th>
            </tr>
          </thead>
          <tbody>
            <% for(let i=0; i<analytic.length; i++){ %>
              <tr>
                <td><%= i+1 %></td>
                <td><%= analytic[i].name %></td>
              </tr>
            <%} %>
          </tbody>
        </table>
      </div>
    </div>
  </div>