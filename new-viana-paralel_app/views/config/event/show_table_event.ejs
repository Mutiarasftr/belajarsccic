<div class="tab-pane fade show active" id="show-event" role="tabpanel" aria-labelledby="home-tab">
    <!-- DataTales Example -->
    <div class="card shadow mb-4 mt-2">
      <div class="card-body">
        <div class="table-responsive">
          <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
            <thead>
              <tr>
                <th>No</th>
                <th>Analytic</th>
                <th>Assigned to Camera</th>
                <th>Description</th>
                <th>Status</th>
                <th>Port</th>
                <th>PID</th>
                <th>Process</th>
                <th>Aksi</th>
              </tr>
            </thead>
            <tbody>
              <% for(let i=0; i<event.length; i++){ %>
                <tr>
                    <td><%= i+1 %></td>
                    <td><%= event[i].analyticId.name %></td>
                    <td><%= event[i].cameraId.name %></td>
                    <td><%= event[i].description %></td>
                    <td><%= event[i].status %></td>
                    <td><%= event[i].port %></td>
                    <td><%= event[i].pid %></td>
                    <td>
                        <% if(event[i].status == 1){ %>
                            <%if(!event[i].process){ %>
                                <a href="/configuration/event/command/start/<%= event[i].id %>" class="btn btn-primary">Start</a>
                            <% }else{ %>
                                <a href="/configuration/event/command/stop/<%= event[i].id %>" class="btn btn-warning">Stop</a>
                            <% } %>
                        <% } %>

                    </td>
                  <td>
                    <form action="/configuration/event/<%= event[i].id %>?_method=DELETE" method="POST">
                      <a href="/configuration/event/<%= event[i].id %>" class="btn btn-warning btn-circle btn-sm button-update" data-id="<%= event[i].id %>"><i class="fas fa-edit"></i></a>
                      <button type="submit" class="btn btn-danger btn-circle btn-sm"><i class="fas fa-trash"></i></button>
                    </form>
                  </td>
                </tr>
              <%} %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  